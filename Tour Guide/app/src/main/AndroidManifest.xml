<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tourguide">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CairoActivity" android:label="Cairo" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".AlexActivity" android:label="ALexandria" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".GizaActivity" android:label="Giza" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".LuxorActivity" android:label="Luxor" android:parentActivityName=".MainActivity"></activity>

    </application>

</manifest>